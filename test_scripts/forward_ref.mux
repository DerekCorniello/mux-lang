// Test forward referencing: mutual recursion
func even(int n) returns bool {
    if n == 0 {
        return true
    } else {
        return odd(n - 1)
    }
}

func odd(int n) returns bool {
    if n == 0 {
        return false
    } else {
        return even(n - 1)
    }
}

func main() returns void {
    print(even(4).to_string())
    print(odd(5).to_string())
    print(even(7).to_string())
    print(odd(8).to_string())
}
