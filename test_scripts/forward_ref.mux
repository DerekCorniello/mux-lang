// Test forward referencing: mutual recursion
fn even(int n) returns bool {
    if n == 0 {
        return true
    } else {
        return odd(n - 1)
    }
}

fn odd(int n) returns bool {
    if n == 0 {
        return false
    } else {
        return even(n - 1)
    }
}

fn main() returns void {
    println(even(4).to_string())
    println(odd(5).to_string())
    println(even(7).to_string())
    println(odd(8).to_string())
}
