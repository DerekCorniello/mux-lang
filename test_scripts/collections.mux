// Collections
auto nums = [1, 2, 3, 4]
auto scores = {"Alice": 90, "Bob": 85}
auto matrix = [[1, 2], [3, 4]]

auto avgs = {"Alice": 90, "Bob": 85}
auto grid = [[1, 2], [3, 4]]
auto myset = {1, 2, 4}

print("nums: " + nums.to_string())
print("scores: " + scores.to_string())
print("matrix: " + matrix.to_string())
print("avgs: " + avgs.to_string())
print("grid: " + grid.to_string())
print("myset: " + myset.to_string())

// Demonstrate both list access methods
print("abt to safe")
print("Safe access: " + nums.get(0).to_string())  // Safe access returns Optional
print("abt to direct")
print("Direct access: " + nums[0].to_string())  // Direct access with runtime error if out of bounds
print("Access a map: " + avgs.get("Alice").to_string())
