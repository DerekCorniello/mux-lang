// Comprehensive Test Suite for All Comparison Operators
//
// This test validates that all comparison operators work correctly
// for all supported types in the Mux language.
//
// Coverage:
// - INT:    ==, !=, <, >, <=, >= (all 6 operators)
// - FLOAT:  ==, !=, <, >, <=, >= (all 6 operators)
// - STRING: ==, != (only equality operators)
// - BOOL:   ==, != (only equality operators)
// - LIST:   ==, != (only equality operators)
// - MAP:    ==, != (only equality operators)
// - SET:    ==, != (only equality operators, order-independent)
//
// Note: Char comparisons (==, !=) are supported but char literals
// are not yet implemented in codegen, so they are not tested here.
//
// Expected: All tests should output "✓" (pass). Any "✗ FAIL" indicates
// a bug in the comparison operator implementation.

func main() returns void {
    println("=== COMPREHENSIVE COMPARISON OPERATOR TESTS ===")
    println("")
    
    // ========== INT COMPARISONS ==========
    println("--- INT COMPARISONS ---")
    
    // Int: == (Equal)
    if 5 == 5 {
        println("✓ int: 5 == 5")
    } else {
        println("✗ FAIL: int: 5 == 5")
    }
    
    if 5 == 3 {
        println("✗ FAIL: int: 5 == 3 should be false")
    } else {
        println("✓ int: 5 == 3 is false")
    }
    
    // Int: != (Not Equal)
    if 5 != 3 {
        println("✓ int: 5 != 3")
    } else {
        println("✗ FAIL: int: 5 != 3")
    }
    
    if 5 != 5 {
        println("✗ FAIL: int: 5 != 5 should be false")
    } else {
        println("✓ int: 5 != 5 is false")
    }
    
    // Int: < (Less Than)
    if 3 < 5 {
        println("✓ int: 3 < 5")
    } else {
        println("✗ FAIL: int: 3 < 5")
    }
    
    if 5 < 3 {
        println("✗ FAIL: int: 5 < 3 should be false")
    } else {
        println("✓ int: 5 < 3 is false")
    }
    
    // Int: > (Greater Than)
    if 5 > 3 {
        println("✓ int: 5 > 3")
    } else {
        println("✗ FAIL: int: 5 > 3")
    }
    
    if 3 > 5 {
        println("✗ FAIL: int: 3 > 5 should be false")
    } else {
        println("✓ int: 3 > 5 is false")
    }
    
    // Int: <= (Less Than or Equal)
    if 3 <= 5 {
        println("✓ int: 3 <= 5")
    } else {
        println("✗ FAIL: int: 3 <= 5")
    }
    
    if 5 <= 5 {
        println("✓ int: 5 <= 5")
    } else {
        println("✗ FAIL: int: 5 <= 5")
    }
    
    if 5 <= 3 {
        println("✗ FAIL: int: 5 <= 3 should be false")
    } else {
        println("✓ int: 5 <= 3 is false")
    }
    
    // Int: >= (Greater Than or Equal)
    if 5 >= 3 {
        println("✓ int: 5 >= 3")
    } else {
        println("✗ FAIL: int: 5 >= 3")
    }
    
    if 5 >= 5 {
        println("✓ int: 5 >= 5")
    } else {
        println("✗ FAIL: int: 5 >= 5")
    }
    
    if 3 >= 5 {
        println("✗ FAIL: int: 3 >= 5 should be false")
    } else {
        println("✓ int: 3 >= 5 is false")
    }
    
    println("")
    
    // ========== FLOAT COMPARISONS ==========
    println("--- FLOAT COMPARISONS ---")
    
    // Float: == (Equal)
    if 5.5 == 5.5 {
        println("✓ float: 5.5 == 5.5")
    } else {
        println("✗ FAIL: float: 5.5 == 5.5")
    }
    
    if 5.5 == 3.3 {
        println("✗ FAIL: float: 5.5 == 3.3 should be false")
    } else {
        println("✓ float: 5.5 == 3.3 is false")
    }
    
    // Float: != (Not Equal)
    if 5.5 != 3.3 {
        println("✓ float: 5.5 != 3.3")
    } else {
        println("✗ FAIL: float: 5.5 != 3.3")
    }
    
    if 5.5 != 5.5 {
        println("✗ FAIL: float: 5.5 != 5.5 should be false")
    } else {
        println("✓ float: 5.5 != 5.5 is false")
    }
    
    // Float: < (Less Than)
    if 3.3 < 5.5 {
        println("✓ float: 3.3 < 5.5")
    } else {
        println("✗ FAIL: float: 3.3 < 5.5")
    }
    
    if 5.5 < 3.3 {
        println("✗ FAIL: float: 5.5 < 3.3 should be false")
    } else {
        println("✓ float: 5.5 < 3.3 is false")
    }
    
    // Float: > (Greater Than)
    if 5.5 > 3.3 {
        println("✓ float: 5.5 > 3.3")
    } else {
        println("✗ FAIL: float: 5.5 > 3.3")
    }
    
    if 3.3 > 5.5 {
        println("✗ FAIL: float: 3.3 > 5.5 should be false")
    } else {
        println("✓ float: 3.3 > 5.5 is false")
    }
    
    // Float: <= (Less Than or Equal)
    if 3.3 <= 5.5 {
        println("✓ float: 3.3 <= 5.5")
    } else {
        println("✗ FAIL: float: 3.3 <= 5.5")
    }
    
    if 5.5 <= 5.5 {
        println("✓ float: 5.5 <= 5.5")
    } else {
        println("✗ FAIL: float: 5.5 <= 5.5")
    }
    
    if 5.5 <= 3.3 {
        println("✗ FAIL: float: 5.5 <= 3.3 should be false")
    } else {
        println("✓ float: 5.5 <= 3.3 is false")
    }
    
    // Float: >= (Greater Than or Equal)
    if 5.5 >= 3.3 {
        println("✓ float: 5.5 >= 3.3")
    } else {
        println("✗ FAIL: float: 5.5 >= 3.3")
    }
    
    if 5.5 >= 5.5 {
        println("✓ float: 5.5 >= 5.5")
    } else {
        println("✗ FAIL: float: 5.5 >= 5.5")
    }
    
    if 3.3 >= 5.5 {
        println("✗ FAIL: float: 3.3 >= 5.5 should be false")
    } else {
        println("✓ float: 3.3 >= 5.5 is false")
    }
    
    println("")
    
    // ========== STRING COMPARISONS ==========
    println("--- STRING COMPARISONS ---")
    
    // String: == (Equal)
    if "hello" == "hello" {
        println("✓ string: \"hello\" == \"hello\"")
    } else {
        println("✗ FAIL: string: \"hello\" == \"hello\"")
    }
    
    if "hello" == "world" {
        println("✗ FAIL: string: \"hello\" == \"world\" should be false")
    } else {
        println("✓ string: \"hello\" == \"world\" is false")
    }
    
    // String: empty strings
    if "" == "" {
        println("✓ string: \"\" == \"\"")
    } else {
        println("✗ FAIL: string: \"\" == \"\"")
    }
    
    // String: != (Not Equal)
    if "hello" != "world" {
        println("✓ string: \"hello\" != \"world\"")
    } else {
        println("✗ FAIL: string: \"hello\" != \"world\"")
    }
    
    if "hello" != "hello" {
        println("✗ FAIL: string: \"hello\" != \"hello\" should be false")
    } else {
        println("✓ string: \"hello\" != \"hello\" is false")
    }
    
    // String: case sensitivity
    if "Hello" != "hello" {
        println("✓ string: \"Hello\" != \"hello\" (case sensitive)")
    } else {
        println("✗ FAIL: string: \"Hello\" != \"hello\" (case sensitive)")
    }
    
    println("")
    
    // ========== BOOL COMPARISONS ==========
    println("--- BOOL COMPARISONS ---")
    
    // Bool: == (Equal)
    if true == true {
        println("✓ bool: true == true")
    } else {
        println("✗ FAIL: bool: true == true")
    }
    
    if false == false {
        println("✓ bool: false == false")
    } else {
        println("✗ FAIL: bool: false == false")
    }
    
    if true == false {
        println("✗ FAIL: bool: true == false should be false")
    } else {
        println("✓ bool: true == false is false")
    }
    
    // Bool: != (Not Equal)
    if true != false {
        println("✓ bool: true != false")
    } else {
        println("✗ FAIL: bool: true != false")
    }
    
    if true != true {
        println("✗ FAIL: bool: true != true should be false")
    } else {
        println("✓ bool: true != true is false")
    }
    
    println("")
    
    // ========== LIST COMPARISONS ==========
    println("--- LIST COMPARISONS ---")
    
    auto list1 = [1, 2, 3]
    auto list2 = [1, 2, 3]
    auto list3 = [1, 2, 4]
    auto list4 = [1, 2]
    
    // List: == (Equal)
    if list1 == list2 {
        println("✓ list: [1, 2, 3] == [1, 2, 3]")
    } else {
        println("✗ FAIL: list: [1, 2, 3] == [1, 2, 3]")
    }
    
    if list1 == list3 {
        println("✗ FAIL: list: [1, 2, 3] == [1, 2, 4] should be false")
    } else {
        println("✓ list: [1, 2, 3] == [1, 2, 4] is false")
    }
    
    if list1 == list4 {
        println("✗ FAIL: list: [1, 2, 3] == [1, 2] should be false")
    } else {
        println("✓ list: [1, 2, 3] == [1, 2] is false (different lengths)")
    }
    
    // List: != (Not Equal)
    if list1 != list3 {
        println("✓ list: [1, 2, 3] != [1, 2, 4]")
    } else {
        println("✗ FAIL: list: [1, 2, 3] != [1, 2, 4]")
    }
    
    if list1 != list4 {
        println("✓ list: [1, 2, 3] != [1, 2]")
    } else {
        println("✗ FAIL: list: [1, 2, 3] != [1, 2]")
    }
    
    if list1 != list2 {
        println("✗ FAIL: list: [1, 2, 3] != [1, 2, 3] should be false")
    } else {
        println("✓ list: [1, 2, 3] != [1, 2, 3] is false")
    }
    
    println("")
    
    // ========== MAP COMPARISONS ==========
    println("--- MAP COMPARISONS ---")
    
    auto map1 = {1: "a", 2: "b"}
    auto map2 = {1: "a", 2: "b"}
    auto map3 = {1: "a", 2: "c"}
    auto map4 = {1: "a"}
    
    // Map: == (Equal)
    if map1 == map2 {
        println("✓ map: {1: \"a\", 2: \"b\"} == {1: \"a\", 2: \"b\"}")
    } else {
        println("✗ FAIL: map: {1: \"a\", 2: \"b\"} == {1: \"a\", 2: \"b\"}")
    }
    
    if map1 == map3 {
        println("✗ FAIL: map: {1: \"a\", 2: \"b\"} == {1: \"a\", 2: \"c\"} should be false")
    } else {
        println("✓ map: {1: \"a\", 2: \"b\"} == {1: \"a\", 2: \"c\"} is false")
    }
    
    if map1 == map4 {
        println("✗ FAIL: map: {1: \"a\", 2: \"b\"} == {1: \"a\"} should be false")
    } else {
        println("✓ map: {1: \"a\", 2: \"b\"} == {1: \"a\"} is false (different sizes)")
    }
    
    // Map: != (Not Equal)
    if map1 != map3 {
        println("✓ map: {1: \"a\", 2: \"b\"} != {1: \"a\", 2: \"c\"}")
    } else {
        println("✗ FAIL: map: {1: \"a\", 2: \"b\"} != {1: \"a\", 2: \"c\"}")
    }
    
    if map1 != map2 {
        println("✗ FAIL: map: {1: \"a\", 2: \"b\"} != {1: \"a\", 2: \"b\"} should be false")
    } else {
        println("✓ map: {1: \"a\", 2: \"b\"} != {1: \"a\", 2: \"b\"} is false")
    }
    
    println("")
    
    // ========== SET COMPARISONS ==========
    println("--- SET COMPARISONS ---")
    
    auto set1 = {1, 2, 3}
    auto set2 = {3, 2, 1}
    auto set3 = {1, 2, 4}
    auto set4 = {1, 2}
    
    // Set: == (Equal - order independent)
    if set1 == set2 {
        println("✓ set: {1, 2, 3} == {3, 2, 1} (order independent)")
    } else {
        println("✗ FAIL: set: {1, 2, 3} == {3, 2, 1} (order independent)")
    }
    
    if set1 == set3 {
        println("✗ FAIL: set: {1, 2, 3} == {1, 2, 4} should be false")
    } else {
        println("✓ set: {1, 2, 3} == {1, 2, 4} is false")
    }
    
    if set1 == set4 {
        println("✗ FAIL: set: {1, 2, 3} == {1, 2} should be false")
    } else {
        println("✓ set: {1, 2, 3} == {1, 2} is false (different sizes)")
    }
    
    // Set: != (Not Equal)
    if set1 != set3 {
        println("✓ set: {1, 2, 3} != {1, 2, 4}")
    } else {
        println("✗ FAIL: set: {1, 2, 3} != {1, 2, 4}")
    }
    
    if set1 != set2 {
        println("✗ FAIL: set: {1, 2, 3} != {3, 2, 1} should be false")
    } else {
        println("✓ set: {1, 2, 3} != {3, 2, 1} is false (order independent)")
    }
    
    println("")
    println("=== ALL COMPARISON TESTS COMPLETED ===")
}
