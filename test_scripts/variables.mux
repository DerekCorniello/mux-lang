// Variable declarations and type inference
const int MAX = 100
int explicit = 42
auto inferred = 4.89
auto name = "Mux"

auto flag = true
auto otherflag = false

print("MAX: 100")
print("explicit: 42")
print("inferred: 4.89")
print("name: Mux")
print("flag: true")
print("otherflag: false")

int counter = 0
print("counter: 0")
counter++
print("counter: 1")
counter++
print("counter: 2")

int countdown = 5
print("countdown: 5")
countdown--
print("countdown: 4")
countdown--
print("countdown: 3")
