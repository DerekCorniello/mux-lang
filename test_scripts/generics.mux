// Generics
func max<T is comparable>(T a, T b) returns T { 
    if a > b { 
        return a
    }
    return b 
}

class Stack<T> {
    list<T> items
    func push(T item) returns void { items.append(item) }
    func pop() returns Optional<T> {
        if items.isEmpty() { return None }
        return Some(items.removeLast())
    }
}

auto s = Stack<int>.new()
s.push(42)
