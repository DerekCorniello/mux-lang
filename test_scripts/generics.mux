// Generics
func max<T is comparable>(T a, T b) returns T { 
    if a > b { 
        return a
    }
    return b 
}

class Stack<T> {
    list<T> items
    func push(T item) returns void { items.push_back(item) }
    func pop() returns Optional<T> {
        if items.is_empty() { return None }
        return Some(items.pop_back())
    }
}

auto s = Stack<int>.new([])
s.push(42)
