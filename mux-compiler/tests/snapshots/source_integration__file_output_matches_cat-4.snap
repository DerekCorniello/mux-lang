"// Comprehensive test for logical operators: !, &&, ||\n// Tests basic operations, precedence, short-circuit evaluation, and edge cases\n\nprint(\"=== Testing NOT Operator (!) ===\")\n\n// Basic NOT\nbool t = true\nbool f = false\n\nbool not_true = !t\nbool not_false = !f\n\nif not_true {\n    print(\"✗ FAIL: !true should be false\")\n} else {\n    print(\"✓ !true = false\")\n}\n\nif not_false {\n    print(\"✓ !false = true\")\n} else {\n    print(\"✗ FAIL: !false should be true\")\n}\n\n// Double negation\nbool double_neg_true = !!t\nbool double_neg_false = !!f\n\nif double_neg_true {\n    print(\"✓ !!true = true\")\n} else {\n    print(\"✗ FAIL: !!true should be true\")\n}\n\nif double_neg_false {\n    print(\"✗ FAIL: !!false should be false\")\n} else {\n    print(\"✓ !!false = false\")\n}\n\n// NOT with literals\nif !true {\n    print(\"✗ FAIL: !true should be false\")\n} else {\n    print(\"✓ !true (literal) = false\")\n}\n\nif !false {\n    print(\"✓ !false (literal) = true\")\n} else {\n    print(\"✗ FAIL: !false should be true\")\n}\n\n// NOT with comparisons\nint x = 5\nint y = 10\n\nif !(x > y) {\n    print(\"✓ !(5 > 10) = true\")\n} else {\n    print(\"✗ FAIL: !(5 > 10) should be true\")\n}\n\nif !(x < y) {\n    print(\"✗ FAIL: !(5 < 10) should be false\")\n} else {\n    print(\"✓ !(5 < 10) = false\")\n}\n\nprint(\"\")\nprint(\"=== Testing AND Operator (&&) ===\")\n\n// Truth table for AND\nbool tt = true && true\nbool tf = true && false\nbool ft = false && true\nbool ff = false && false\n\nif tt {\n    print(\"✓ true && true = true\")\n} else {\n    print(\"✗ FAIL: true && true should be true\")\n}\n\nif tf {\n    print(\"✗ FAIL: true && false should be false\")\n} else {\n    print(\"✓ true && false = false\")\n}\n\nif ft {\n    print(\"✗ FAIL: false && true should be false\")\n} else {\n    print(\"✓ false && true = false\")\n}\n\nif ff {\n    print(\"✗ FAIL: false && false should be false\")\n} else {\n    print(\"✓ false && false = false\")\n}\n\n// AND with comparisons\nif (x < y) && (y > 0) {\n    print(\"✓ (5 < 10) && (10 > 0) = true\")\n} else {\n    print(\"✗ FAIL: (5 < 10) && (10 > 0) should be true\")\n}\n\nif (x > y) && (y > 0) {\n    print(\"✗ FAIL: (5 > 10) && (10 > 0) should be false\")\n} else {\n    print(\"✓ (5 > 10) && (10 > 0) = false\")\n}\n\n// Chained AND\nif true && true && true {\n    print(\"✓ true && true && true = true\")\n} else {\n    print(\"✗ FAIL: true && true && true should be true\")\n}\n\nif true && true && false {\n    print(\"✗ FAIL: true && true && false should be false\")\n} else {\n    print(\"✓ true && true && false = false\")\n}\n\nprint(\"\")\nprint(\"=== Testing OR Operator (||) ===\")\n\n// Truth table for OR\nbool or_tt = true || true\nbool or_tf = true || false\nbool or_ft = false || true\nbool or_ff = false || false\n\nif or_tt {\n    print(\"✓ true || true = true\")\n} else {\n    print(\"✗ FAIL: true || true should be true\")\n}\n\nif or_tf {\n    print(\"✓ true || false = true\")\n} else {\n    print(\"✗ FAIL: true || false should be true\")\n}\n\nif or_ft {\n    print(\"✓ false || true = true\")\n} else {\n    print(\"✗ FAIL: false || true should be true\")\n}\n\nif or_ff {\n    print(\"✗ FAIL: false || false should be false\")\n} else {\n    print(\"✓ false || false = false\")\n}\n\n// OR with comparisons\nif (x > y) || (y > 0) {\n    print(\"✓ (5 > 10) || (10 > 0) = true\")\n} else {\n    print(\"✗ FAIL: (5 > 10) || (10 > 0) should be true\")\n}\n\nif (x > y) || (y < 0) {\n    print(\"✗ FAIL: (5 > 10) || (10 < 0) should be false\")\n} else {\n    print(\"✓ (5 > 10) || (10 < 0) = false\")\n}\n\n// Chained OR\nif false || false || true {\n    print(\"✓ false || false || true = true\")\n} else {\n    print(\"✗ FAIL: false || false || true should be true\")\n}\n\nif false || false || false {\n    print(\"✗ FAIL: false || false || false should be false\")\n} else {\n    print(\"✓ false || false || false = false\")\n}\n\nprint(\"\")\nprint(\"=== Testing Short-Circuit Evaluation ===\")\n\n// Helper functions to test side effects\nfunc should_not_run_and() returns bool {\n    print(\"✗ ERROR: should_not_run_and() was called (short-circuit failed)\")\n    return true\n}\n\nfunc should_run_and() returns bool {\n    print(\"  should_run_and() called (expected)\")\n    return true\n}\n\nfunc should_not_run_or() returns bool {\n    print(\"✗ ERROR: should_not_run_or() was called (short-circuit failed)\")\n    return false\n}\n\nfunc should_run_or() returns bool {\n    print(\"  should_run_or() called (expected)\")\n    return false\n}\n\n// Test AND short-circuit: false && func() should NOT call func()\nprint(\"Testing: false && should_not_run_and()\")\nbool and_sc1 = false && should_not_run_and()\nprint(\"✓ Short-circuit AND: right side not evaluated when left is false\")\n\n// Test AND non-short-circuit: true && func() SHOULD call func()\nprint(\"Testing: true && should_run_and()\")\nbool and_sc2 = true && should_run_and()\nprint(\"✓ AND evaluates right side when left is true\")\n\n// Test OR short-circuit: true || func() should NOT call func()\nprint(\"Testing: true || should_not_run_or()\")\nbool or_sc1 = true || should_not_run_or()\nprint(\"✓ Short-circuit OR: right side not evaluated when left is true\")\n\n// Test OR non-short-circuit: false || func() SHOULD call func()\nprint(\"Testing: false || should_run_or()\")\nbool or_sc2 = false || should_run_or()\nprint(\"✓ OR evaluates right side when left is false\")\n\nprint(\"\")\nprint(\"=== Testing Mixed Operators and Precedence ===\")\n\n// NOT has higher precedence than AND\nbool a = true\nbool b = false\nbool c = true\n\n// !b && c should be (!b) && c = true && true = true\nif !b && c {\n    print(\"✓ !false && true = true (precedence: NOT before AND)\")\n} else {\n    print(\"✗ FAIL: !false && true should be true\")\n}\n\n// AND has higher precedence than OR\n// false || true && true should be false || (true && true) = false || true = true\nif false || true && true {\n    print(\"✓ false || true && true = true (precedence: AND before OR)\")\n} else {\n    print(\"✗ FAIL: false || true && true should be true\")\n}\n\n// Complex expression: !(a && b) || c\n// !(true && false) || true = !false || true = true || true = true\nif !(a && b) || c {\n    print(\"✓ !(true && false) || true = true\")\n} else {\n    print(\"✗ FAIL: !(true && false) || true should be true\")\n}\n\n// De Morgan's law: !(a && b) should equal (!a || !b)\nbool demorgan1 = !(a && b)\nbool demorgan2 = !a || !b\nif demorgan1 == demorgan2 {\n    print(\"✓ De Morgan's law: !(a && b) == (!a || !b)\")\n} else {\n    print(\"✗ FAIL: De Morgan's law failed\")\n}\n\n// De Morgan's law: !(a || b) should equal (!a && !b)\nbool demorgan3 = !(a || b)\nbool demorgan4 = !a && !b\nif demorgan3 == demorgan4 {\n    print(\"✓ De Morgan's law: !(a || b) == (!a && !b)\")\n} else {\n    print(\"✗ FAIL: De Morgan's law failed\")\n}\n\n// Complex nested expression\nbool complex = (a && !b) || (!a && b)\nif complex {\n    print(\"✓ (true && !false) || (!true && false) = true\")\n} else {\n    print(\"✗ FAIL: (true && !false) || (!true && false) should be true\")\n}\n\nprint(\"\")\nprint(\"=== Testing Logical Operators in Control Flow ===\")\n\n// In if conditions\nint result = 0\nif x < 10 && y > 5 {\n    result = 1\n}\nif result == 1 {\n    print(\"✓ Logical AND in if condition works\")\n} else {\n    print(\"✗ FAIL: Logical AND in if condition failed\")\n}\n\n// In while loops\nint counter = 0\nbool flag = true\nwhile counter < 3 && flag {\n    counter = counter + 1\n}\nif counter == 3 {\n    print(\"✓ Logical AND in while condition works\")\n} else {\n    print(\"✗ FAIL: Logical AND in while condition failed\")\n}\n\nprint(\"\")\nprint(\"=== All Logical Operator Tests Completed ===\")\n"
